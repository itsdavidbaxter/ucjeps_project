<?xml version="1.0" encoding="UTF-8"?>
<document name="taxon">
  <ns2:taxon_common xmlns:ns2="http://collectionspace.org/services/taxonomy" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <taxonTermGroupList>
      <taxonTermGroup>
        <termName>{{termName}}</termName>
        <termSource>{{termSource}}</termSource>
        <termSourceID>{{termSourceID}}</termSourceID>
        <taxonomicStatus>{{taxonomicStatus}}</taxonomicStatus>
        <termPrefForLang>{{termPrefForLang}}</termPrefForLang>
        <termDisplayName>{{termDisplayName}}</termDisplayName>
        <termType>{{termType}}</termType>
        <termStatus>{{termStatus}}</termStatus>
        <termFormattedDisplayName>{{termFormattedDisplayName}}</termFormattedDisplayName>
      </taxonTermGroup>
    </taxonTermGroupList>
    <taxonCurrency>{{taxonCurrency}}</taxonCurrency>
    <commonNameGroupList>
      <commonNameGroup>
        <commonName>{{commonName}}</commonName>
        <commonNameLanguage/>
        <commonNameSource/>
        <commonNameSourceDetail/>
      </commonNameGroup>
    </commonNameGroupList>
    <inAuthority>{{inAuthority}}</inAuthority>
    <taxonIsNamedHybrid>{{taxonIsNamedHybrid}}</taxonIsNamedHybrid>
    <taxonNote/>
    <taxonRank>{{taxonRank}}</taxonRank>
  </ns2:taxon_common>
  <ns2:taxon_naturalhistory xmlns:ns2="http://collectionspace.org/services/taxon/domain/naturalhistory" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <plantAttributesGroupList/>
    <naturalHistoryCommonNameGroupList/>
    <taxonBasionym/>
    <family>{{family}}</family>
    <relatedTermGroupList/>
  </ns2:taxon_naturalhistory>
  <ns2:taxon_ucjeps xmlns:ns2="http://collectionspace.org/services/taxon/local/ucjeps" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <taxonMajorGroup>{{taxonMajorGroup}}</taxonMajorGroup>
  </ns2:taxon_ucjeps>
</document>
